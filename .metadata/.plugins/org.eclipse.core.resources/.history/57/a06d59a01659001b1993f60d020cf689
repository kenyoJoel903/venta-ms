package com.kenyo.productos;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.cloud.netflix.eureka.EnableEurekaClient;
import org.springframework.context.annotation.ComponentScan;
import org.springframework.context.annotation.Import;

import com.kenyo.SecurityConfiguration;
import com.kenyo.SwaggerConfig;
import com.kenyo.seguridad.AuthEntryPointJwt;
import com.kenyo.seguridad.JwtAutenticationFilter;
import com.kenyo.seguridad.JwtUtils;

@SpringBootApplication
@EnableEurekaClient
@Import({SwaggerConfig.class, SecurityConfiguration.class, JwtAutenticationFilter.class, AuthEntryPointJwt.class, JwtUtils.class})
@ComponentScan(basePackages = "com.kenyo")
public class MsproductosApplication {

	public static void main(String[] args) {
		SpringApplication.run(MsproductosApplication.class, args);
	}

}
